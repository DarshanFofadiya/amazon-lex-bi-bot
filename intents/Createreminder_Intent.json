{
    "name": "Createreminder_Intent",
    "description": "Create Reminder to follow up",
    "sampleUtterances": [
        "Create a reminder to follow up with {merchant} next {time1}",
        "set a reminder to follow up next {time1}",
        "set a reminder"
    ],
    "slots": [
      {
            "slotType": "merchant",
            "name": "merchant",
            "slotConstraint": "Optional",
            "valueElicitationPrompt": {
                "maxAttempts": 2,
                "messages": [
                    {
                        "content": "what is the merchant id",
                        "contentType": "PlainText"
                    }
                ]
            },
            "priority": 1,
            "slotTypeVersion": "$LATEST",
            "sampleUtterances": []
      },
      {
        "slotType": "time1",
        "name": "time1",
        "slotConstraint": "Optional",
        "valueElicitationPrompt": {
          "maxAttempts": 2,
          "messages": [
            {
              "content": "when do you want to set the reminder for",
              "contentType": "PlainText"
            }
          ]
        },
        "priority": 2,
        "slotTypeVersion": "$LATEST",
        "sampleUtterances": []
      }
    ],
    "conclusionStatement": {
      "messages": [
        {
          "content": "via code hook",
          "contentType": "PlainText"
        }
      ]
    },
    "fulfillmentActivity": {
        "type": "CodeHook",
        "codeHook": {
            "uri": "{{lambda-arn}}",
            "messageVersion": "1.0"
        }
    }
}